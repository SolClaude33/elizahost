{
  "name": "amica-agent",
  "version": "1.0.0",
  "description": "AMICA Agent powered by ElizaOS on Railway",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node scripts/check-env.js && elizaos start --character ./characters/amica-agent.json",
    "start:manual": "tsx index.ts",
    "dev": "elizaos dev",
    "build": "node scripts/copy-assets.js",
    "type-check": "tsc --noEmit",
    "extract-solana-key": "node scripts/extract-solana-private-key.js"
  },
  "keywords": [
    "elizaos",
    "amica",
    "ai",
    "solana",
    "blockchain"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@elizaos/core": "latest",
    "@elizaos/plugin-bootstrap": "latest",
    "@elizaos/plugin-solana": "latest",
    "@elizaos/plugin-twitter": "latest",
    "@elizaos/plugin-openai": "latest",
    "@elizaos/cli": "latest",
    "@solana/web3.js": "^1.87.6",
    "bs58": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
